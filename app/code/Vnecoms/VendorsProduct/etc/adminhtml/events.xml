<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="ves_vendors_admin_notifications">
        <observer name="vnecoms_vendorsproduct_admin_notifications" instance="Vnecoms\VendorsProduct\Observer\PendingProductObserver" />
    </event>
    <event name="catalog_product_attribute_update_before">
        <observer name="vnecoms_vendorsproduct_product_attribute_update_before" instance="Vnecoms\VendorsProduct\Observer\UpdateVendorObserver" />
    </event>

    <event name="vendor_delete_after">
        <observer name="vnecoms_vendors_pricecomparison_vendor_delete_after" instance="Vnecoms\VendorsProduct\Observer\VendorDeleteAfter" />
    </event>

</config>
